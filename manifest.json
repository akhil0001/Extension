{
    "manifest_version":2,
    "name":"Shazam",
    "version":"0.1",
    
    "description": "This stores your all things that have to be completed",
    "homepage_url":"https://github.com/akhil0001/Extension",
    "icons":{
        "48":"icons/web/checked_1x.png",
        "96":"icons/web/checked_2x.png"
    },
    "permissions":[
        "storage",
        "alarms",
       "notifications"
    ],
    "browser_action":{
        "default_icon":"icons/web/checked_1x.png",
        "default_title":"Shazam",
        "default_popup":"todo/popup.html"
    },
    "background":{
        "scripts":["todo/background-script.js"]
    }

}